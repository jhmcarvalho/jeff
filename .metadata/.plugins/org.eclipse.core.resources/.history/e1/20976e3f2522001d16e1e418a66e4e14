package aula_dev;

import java.text.DateFormat;
import java.text.SimpleDateFormat;
import java.util.Calendar;

public class DataHora {

	public static void main(String[] args) {
		Calendar novoCalendario = Calendar.getInstance();
		SimpleDateFormat dataHora = new SimpleDateFormat("dd/MM/yyyy - hh : mm : ss");
		DateFormat dataHoraFormatada = null;
		dataHoraFormatada.format(novoCalendario.getTime());
		System.out.println(dataHoraFormatada);

	}

}
